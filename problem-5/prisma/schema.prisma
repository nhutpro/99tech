datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  gender    String   @default("male")
  role      String   @default("user")
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
